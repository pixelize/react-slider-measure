    "@types/react": "^16.9.14",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.0",
    "concurrently": "^5.0.0",
    "gatsby": "^2.18.6",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "prettier": "^1.19.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
